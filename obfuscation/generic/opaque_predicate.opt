group "obfuscation" bool_option opaque_predicates
{
  short="oop";
  long="obfuscationOpaquePredicates";
  description="Insert opaque predicates";
  default=off;
}

group "obfuscation" int_option opaque_predicate_chance
{
  short="oopchance";
  long="obfuscationOpaquePredicateChance";
  description="Percent chance of inserting an opaque predicate";
  default=50;
}

usage_option help
{
   long="help";
   short="h";
   description="Print this help message";
}
